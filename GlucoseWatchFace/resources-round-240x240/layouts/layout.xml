<layout id="WatchFace">
    <drawable id="BackgroundDark" visible="false" />
    <drawable id="BackgroundLight" visible="true" />
    <drawable id="DateValueGraph" class="Shared.Graph">
      <param name="x">3</param>
      <param name="y">120</param>
      <param name="width">234</param>
       <param name="height">86</param>
    </drawable>      
    <label id="DateLabel" x="center" y="10" 
