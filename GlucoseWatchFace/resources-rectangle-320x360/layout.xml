<resources>
<layout id="WatchFace">
    <drawable id="BackgroundDark" visible="true" />
    <drawable id="BackgroundLight" visible="false" />
    <drawable id="DateValueGraph" class="Shared.Graph">
      <param name="x">0</param>
      <param name="y">190</param>
      <param name="width">320</param>
