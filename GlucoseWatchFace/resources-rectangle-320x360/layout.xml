    <label id="MessageLabel" x="20" y="160"
           font="Graphics.FONT_SYSTEM_XTINY"
           justification="Graphics.TEXT_JUSTIFY_LEFT"
           color="Graphics.COLOR_RED" />
</layout>
</resources>
