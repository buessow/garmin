    <label id="Data3Label" x="110" y="119"
           font="Graphics.FONT_SYSTEM_XTINY"
           justification="Graphics.TEXT_JUSTIFY_RIGHT"
           color="Graphics.COLOR_WHITE" />
    <label id="Data4Label" x="110" y="148"
           font="Graphics.FONT_SYSTEM_XTINY"
           justification="Graphics.TEXT_JUSTIFY_RIGHT"
